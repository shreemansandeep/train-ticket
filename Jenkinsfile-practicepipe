pipeline{
agent any

  stages{

    stage("Git Checkout"){
      steps{            
              git credentialsId: 'GitID', url: 'https://github.com/shreemansandeep/Train-Ticket-Reservation-System.git'
              }
           }
           
     stage("Unit testing"){
      steps{
              sh 'mvn test'
              }
           }
           
      stage("Inregration testing"){
        steps{
              sh 'mvn verify -DskipUnitTests'
            }
          }     

        }
    }
